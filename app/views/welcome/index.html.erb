<h1>Web Dev Club</h1>
<%- if user_signed_in? %>
 <%= link_to 'New post', new_article_path %>
<% else %>
 
<% end %>


<table>
  <tr>
    <th>Title</th>
    <th>Author</th>
  </tr>
 
  <% @articles.each do |article| %>
    <tr>
      <td><%= link_to article.title, article_path(article)%></td>
      <td><%= article.username %></td>
      <td><%= article.created_at.strftime("%m/%d/%Y") %></td>

    </tr>
  <% end %>


  <%- if user_signed_in? %>

    <% @attends.each do |attend| %>
        <tr>
          <td><%= attend.attendee %></td>

        </tr>
      <% end %>
  <%- end %>
</table>